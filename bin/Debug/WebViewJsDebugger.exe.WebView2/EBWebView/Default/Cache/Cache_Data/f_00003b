ListFocused, setIsListFocused] = useState(false);\n  const [selectedLanguage, setSelectedLanguage] = useState<Language>(null);\n  const [isLoggedUser] = useConnect(sessionStore.isLogged$);\n  const { translate } = useTranslation();\n  const {\n    focusNextLanguage,\n    focusPreviousLanguage,\n    focusFirstLanguageElement,\n  } = useLanguageSelector(languagesRef, focusedItem);\n\n  const isLiveTranslationAvailable = useMemo(\n    () =>\n      paidProductsEnabled.LIVE_TRANSLATIONS &&\n      liveTranslationsOptions.is_enabled &&\n      liveTranslationsOptions.translationAllowed,\n    [liveTranslationsOptions, paidProductsEnabled.LIVE_TRANSLATIONS],\n  );\n\n  const closeLanguageSelector = () => {\n    setIsListFocused(false);\n    setShowLanguageMenu(false);\n    selectedLanguageFocus.current = true;\n  };\n  const { sendNavigationReaderRequest } = useNavigationReader();\n  const closeLanguageSelectorWithEsc = (event: KeyboardEvent) => {\n    if (event.code === 'Escape') {\n      stopEvents(event);\n      closeLanguageSelector();\n    }\n  };\n\n  const readContent = (e: InputEvent) => {\n    const label = e.data;\n\n    if (label) {\n      sendNavigationReaderRequest(label);\n    }\n    if (e.inputType === 'deleteContentBackward') {\n      sendNavigationReaderRequest(translate('widget.language.backspace'));\n    }\n    if (e.inputType === 'deleteContentForward') {\n      sendNavigationReaderRequest(translate('widget.language.delete'));\n    }\n  };\n  useEffect(() => {\n    if (!widgetPageLang || !LANGUAGES) {\n      return;\n    }\n\n    originalPageLang.current = getLanguageData(widgetPageLang, LANGUAGES);\n  }, [widgetPageLang]);\n\n  useEffect(() => {\n    const findLanguage = getLanguageData(language, LANGUAGES);\n    setSelectedLanguage(findLanguage);\n  }, [language]);\n\n  useEffect(() => {\n    if (showLanguageMenu) {\n      window.addEventListener('keydown', closeLanguageSelectorWithEsc, true);\n    } else {\n      window.removeEventListener('keydown', cl