lement.scrollIntoView(true);\n      return scrollContainer.scroll(0, scrollContainer.scrollTop - 10);\n    }\n\n    if (isHiddenBy === HiddenBySection.Bottom) {\n      // scroll to the end of the element and add 10 px down\n      const targetBound = element.getBoundingClientRect();\n      const scrollBound = scrollContainer.getBoundingClientRect();\n      const scrollDiff = targetBound.bottom - scrollBound.bottom;\n\n      return scrollContainer.scroll(0, scrollContainer.scrollTop + scrollDiff + 10);\n    }\n  }\n}\n\n(window as any).layoutStore = new LayoutStore();\n\nexport const layoutStore = (window as any).layoutStore as LayoutStore;\n","import { INITIAL_PAGE_STATE } from 'constants/constants';\nimport { stateStore } from 'global/state.store';\nimport { sendPostMessage } from 'helpers/post-message.helper';\nimport { FunctionComponent, h } from 'preact';\nimport { useContext, useEffect, useMemo, useState } from 'preact/hooks';\nimport { useApi } from 'services/use-api';\nimport { ApplicationContext } from 'services/use-application-setup';\nimport { useTranslation } from 'services/use-translation';\nimport './report.scss';\nimport { isMobile } from 'helpers/detect-user-agent';\nimport { layoutStore } from '../../global/layout.store';\n\nconst NO_SCREENSHOT = 'no_screenshot_available';\n\nexport const ReportView: FunctionComponent = () => {\n  const { code, href, settings } = useContext(ApplicationContext);\n  const [viewportScreenshot, setViewportScreenshot] = useState('');\n  const [isProcessing, setIsProcessing] = useState(false);\n  const [formError, setFormError] = useState<{ email: string; text: string }>(null);\n\n  const [text, setText] = useState('');\n  const [email, setEmail] = useState('');\n\n  const Api = useApi();\n  const { translate } = useTranslation();\n\n  const goBack = () => {\n    stateStore.state.next(INITIAL_PAGE_STATE);\n  };\n\n  const clearErrors = () => {\n    setFormError(null);\n  };\n\n  useEffect(() => {\n    const handleScreenshotPreview = (event) => {\n      const data = event.data || {};\n\n      if (data.action === 'sendScreenShotPreview') {\n        setViewportScreenshot(data.imageSrc || NO_SCREENSHOT);\n      }\n    };\n\n    window.addEventListener('message', handleScreenshotPreview);\n\n    // send message to take screen-shot\n    sendPostMessage({\n      action: 'takeScreenShot',\n    });\n\n    return () => window.removeEventListener('message', handleScreenshotPreview);\n  }, []);\n\n  const isScreenshotAvailable = useMemo(\n    () => !isMobile() && viewportScreenshot && viewportScreenshot !== NO_SCREENSHOT,\n    [viewportScreenshot],\n  );\n\n  const dataURItoBlob = (dataURI: string) => {\n    let byteString;\n    if (dataURI.split(',')[0].indexOf('base64') >= 0) {\n      byteString = atob(dataURI.split(',')[1]);\n    } else {\n      byteString = unescape(dataURI.split(',')[1]);\n    }\n    const mimeString = dataURI.split(',')[0].split(':')[1].split(';')[0];\n    const ia = new Uint8Array(byteString.length);\n    for (let i = 0; i < byteString.length; i++) {\n      ia[i] = byteString.charCodeAt(i);\n    }\n    return new Blob([ia], { type: mimeString });\n  };\n\n  const getActiveFeature = () => {\n    const allFeatures = settings.current;\n\n    return Object.keys(allFeatures).reduce((features, key) => {\n      if (allFeatures[key].value) {\n        features[key] = true;\n      }\n\n      return features;\n    }, {});\n  };\n\n  const submitForm = (event) => {\n    event.preventDefault();\n    setIsProcessing(true);\n\n    const formData = new FormData();\n    if (code) {\n      formData.append('code', code);\n    }\n    if (href) {\n      formData.append('url', href);\n    }\n    if (text) {\n      formData.append('text', text);\n    }\n    if (email) {\n      formData.append('email', email);\n    } else {\n      formData.append('email', '');\n    }\n    if (settings) {\n      formData.append('features', JSON.stringify(getActiveFeature()));\n    }\n    if (viewportScreenshot && viewportScreenshot !== NO_SCREENSHOT) {\n      formData.append('screenShot', dataURItoBlob(viewportScreenshot));\n    }\n\n    return Api.sendFormData('reports', formData)\n      .then((response) => {\n        clearErrors();\n        if (response.errors) {\n          setFormError(response.errors);\n        }\n        if (response.code === 200) {\n          setText('');\n\n          // $rootScope.$broadcast(\"showMessage\", {\n          //   type: \"success\",\n          //   message: \"widget.report.saved\",\n          // });\n          goBack();\n        }\n      })\n      .catch((error) => {\n        setFormError(error.errors);\n        // $rootScope.$broadcast(\"showMessage\", {\n        //   type: \"error\",\n        //   body: error.message,\n        // });\n      })\n      .finally(() => {\n        setIsProcessing(false);\n      });\n  };\n\n  const onFocus = (e) => {\n    e.target.parentElement && e.target.parentElement.classList.add('focus');\n  };\n  const onBlur = (e) => {\n    e.target.parentElement && e.target.parentElement.classList.remove('focus');\n  };\n  useEffect(() => layoutStore.viewUpdate$.next(), []);\n  if (!viewportScreenshot) {\n    return (\n      <div className=\"widget-form widget-form_loading\">\n        <img src=\"./frontend/images/spinner.svg\" alt=\"\" width={40} aria-hidden=\"true\" />\n      </div>\n    );\n  }\n\n  return (\n    <div class=\"widget-form widget-form_report\">\n      <form name=\"form\" role=\"form\" onSubmit={submitForm} noValidate autocomplete=\"off\">\n        <fieldset disabled={isProcessing}>\n          <div\n            input-focus\n            auto-focus=\"true\"\n            class={`widget-input ${formError?.text ? 'has-error' : ''}`}\n          >\n            <label id=\"report-txt\" class=\"label\" for=\"text\">\n              {translate('widget.report.text.label')}\n            </label>\n            <textarea\n              onInput={(e) => setText((e.target as HTMLInputElement).value)}\n              onFocus={onFocus}\n              onBlur={onBlur}\n              aria-labelledby=\"report-txt report-txt-err\"\n              aria-invalid={formError?.text ? 'true' : 'false'}\n              title=\"{{ 'widget.report.text.label' | translate }}\"\n              id=\"text\"\n              name=\"text\"\n              class=\"input input_textarea\"\n            />\n            {formError?.text && (\n              <div id=\"report-txt-err\" role=\"alert\" class=\"help-block\">\n                {translate(formError.text)}\n              </div>\n            )}\n          </div>\n          <div input-focus class={`widget-input ${formError?.email ? 'has-error' : ''}`}>\n            <label id=\"report-email\" class=\"label\" for=\"email\">\n              {translate('widget.report.email.label')}\n            </label>\n            <input\n              onInput={(e) => setEmail((e.target as HTMLInputElement).value)}\n              onFocus={onFocus}\n              onBlur={onBlur}\n              aria-labelledby=\"report-email report-email-err\"\n              aria-invalid={formError?.email ? 'true' : 'false'}\n              title={translate('widget.report.email.label')}\n              id=\"email\"\n              name=\"email\"\n              class=\"input\"\n              placeholder={translate('widget.report.email.placeholder')}\n              type=\"email\"\n            />\n            {formError?.email && (\n              <div id=\"report-email-err\" role=\"alert\" class=\"help-block\">\n                {translate(formError.email)}\n              </div>\n            )}\n          </div>\n\n          <div class=\"buttons\">\n            <button\n              class=\"btn btn_blue btn_border\"\n              onClick={goBack}\n              type=\"button\"\n              aria-label={translate('widget.report.buttons.cancel.aria')}\n            >\n              {translate('widget.report.buttons.cancel.label')}\n            </button>\n            <button\n              class={`btn btn_blu