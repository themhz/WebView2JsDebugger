 browsers.\n    textArea.style.width = '2em';\n    textArea.style.height = '2em';\n\n    // We don't need padding, reducing the size if it does flash render.\n    textArea.style.padding = '0';\n\n    // Clean up any borders.\n    textArea.style.border = 'none';\n    textArea.style.outline = 'none';\n    textArea.style.boxShadow = 'none';\n\n    // Avoid flash of white box if rendered for any reason.\n    textArea.style.background = 'transparent';\n\n    textArea.value = text;\n\n    document.body.appendChild(textArea);\n    textArea.focus();\n    textArea.select();\n\n    try {\n      const successful = document.execCommand('copy');\n      const msg = successful ? 'successful' : 'unsuccessful';\n      console.log(`Copying text command was ${msg}`);\n    } catch (err) {\n      console.log('Oops, unable to copy');\n    }\n\n    document.body.removeChild(textArea);\n  }, []);\n\n  const keyDown = useCallback(\n    (event) => {\n      if (event.keyCode === 76 && event.ctrlKey) {\n        // CTRL + L\n        event.preventDefault();\n        stateStore.state.next('main.login');\n      }\n\n      if (isHotkeyToOpenWidget(config, event) || event.keyCode === 27) {\n        // check if tooltip in widget is focused\n        const activeElement = document.activeElement as HTMLButtonElement;\n        const isTooltipFocused = activeElement?.classList?.contains('uwaw-main-tooltip-icon');\n        if (isTooltipFocused && event.keyCode === 27) {\n          // hide tooltip on first Esc key press\n          activeElement.blur();\n        } else {\n          // CTRL + U - default; or Escape\n          event.preventDefault();\n          config.close();\n        }\n      }\n\n      if (event.keyCode === 75 && event.ctrlKey) {\n        // CTRL + K\n        event.preventDefault();\n        const copyText = config.code;\n        copyTextToClipboard(copyText);\n      }\n\n      if (event.keyCode === 74 && event.ctrlKey) {\n        // CTRL + J\n        event.preventDefault();\n        const copyText = config.config.current.services.userId;\n        copyTextToClipboard(copyText);\n      }\n    },\n    [copyTextToClipboard, config],\n  );\n\n  useEffect(() => {\n    if (config.isOpen) {\n      window.addEventListener('keydown', keyDown);\n    }\n\n    return () => {\n      window.removeEventListener('keydown', keyDown);\n    };\n  }, [keyDown, config?.isOpen]);\n};\n","import { Subject } from 'helpers/store';\nimport { INITIAL_PAGE_STATE } from 'constants/constants';\n\nexport const HOME_TITLE = 'widget.home.title';\n\nclass HeaderStore {\n  public clickBack = new Subject();\n\n  public backState = new Subject({ replay: true, initialState: '' });\n\n  public title = new Subject({ replay: true, initialState: HOME_TITLE });\n\n  public isMainMenu = new Subject({ replay: true, initialState: true });\n\n  setBackStateToHomePage() {\n    this.backState.next(INITIAL_PAGE_STATE);\n  }\n\n  setIsMainMenu(value: boolean) {\n    this.isMainMenu.next(value);\n  }\n}\n\n(window as any).headerStore = new HeaderStore();\n\nexport const headerStore = (window as any).headerStore as HeaderStore;\n","import { Subject } from 'helpers/store';\nimport { ALLY_FEATURES } from 'constants/ally-features';\n\nexport interface ResetFeatureData {\n  trigger: {\n    feature: string;\n    actionState: number;\n  };\n}\n\nclass AllyFeaturesStore {\n  public resetFeature$ = new Subject<ResetFeatureData>();\n\n  public get features() {\n    return Object.keys(ALLY_FEATURES)\n      .filter((key) => !ALLY_FEATURES[key].isProfile)\n      .reduce((res, key) => {\n        res[key] = ALLY_FEATURES[key];\n        return res;\n      }, {});\n  }\n\n  public get profiles() {\n    return Object.keys(ALLY_FEATURES)\n      .filter((key) => ALLY_FEATURES[key].isProfile)\n      .reduce((res, key) => {\n        res[key] = ALLY_FEATURES[key];\n        return res;\n      }, {});\n  }\n}\n\n(window as any).allyFeaturesStore = new AllyFeaturesStore();\n\nexport const allyFeaturesStore = (window as any).all